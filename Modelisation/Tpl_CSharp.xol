<a:TemplateTargetItem.Value>using System;
using AppModel.Entity;
using Lib;

namespace %GenOptions.namespace%.Association
{
    public class %Name% : IEntityAssociation
    {
        public %Name%(%.FU:ClassA.Code% %.L:ClassA.Code%,%.FU:ClassB.Code% %.L:ClassB.Code%)
        {
            this.%.FU:ClassA.Code% = %.L:ClassA.Code%;
            this.%.FU:ClassB.Code% = %.L:ClassB.Code%;
        }

        public %Name%(%.FU:ClassB.Code% %.L:ClassB.Code%, %.FU:ClassA.Code% %.L:ClassA.Code%)
        {
            this.%.FU:ClassA.Code% = %.L:ClassA.Code%;
            this.%.FU:ClassB.Code% = %.L:ClassB.Code%;
        }

        public %.FU:ClassA.Code% %.FU:ClassA.Code%;
        public %.FU:ClassB.Code% %.FU:ClassB.Code%;
        public IModel model;

        public IEntity A
        {
            get { return %.FU:ClassA.Code%; }
        }

        public IEntity B
        {
            get { return %.FU:ClassB.Code%; }
        }

        public IModel Model
        {
            get
            {
                return model;
            }
            set
            {
                model = value;
            }
