<a:TemplateTargetItem.Value>public class %Code%Converter : IValueConverter
{

  %items%
   
  public object Convert(object value, Type targetType, object parameter, CultureInfo culture)
  {
      if (value == null)
          return null;


.if(%.L:DataType%!=&quot;string&quot;)
      %DataType% val;
      if (%DataType%.TryParse(value.ToString(), out val) == false)
          return null;
.else
      string val = value.ToString();
.endif


      switch (val)
      {

.set_value(first,true,new)
.set_value(name,null,new)
.set_value(value,null,new)
.foreach_line (%ListOfValues%)
.foreach_part (%CurrentLine%,&#39;\t&#39;)
.if(%first%==true)
.set_value(value,%CurrentPart%,update)
.set_value(first,false,update)
.else
.set_value(name,%CurrentPart%,update)
.set_value(first,true,update)
.endif
.next
          case %Code%.
.convert_name(%name%,&quot;_&quot;,&quot; &quot;,FirstUpperChar)
:
              return &quot;%name%&quot;;
.next(\n)

      }

      return null;
  }
  
  public object ConvertBack(object value, Type targetType, object parameter, CultureInfo culture)
  {
      if (value == null)
          return null;

      switch (value.ToString())
      {

.set_value(first,true,new)
.set_value(name,null,new)
.set_value(value,null,new)
.foreach_line (%ListOfValues%)
.foreach_part (%CurrentLine%,&#39;\t&#39;)
.if(%first%==true)
.set_value(value,%CurrentPart%,update)
.set_value(first,false,update)
.else
.set_value(name,%CurrentPart%,update)
.set_value(first,true,update)
.endif
.next
          case &quot;%name%&quot;:
              return %Code%.
.convert_name(%name%,&quot;_&quot;,&quot; &quot;,FirstUpperChar)
;
.next(\n)

      }
      return null;
  }
}</a:TemplateTargetItem.Value>
