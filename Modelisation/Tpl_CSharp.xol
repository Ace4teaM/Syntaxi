<a:TemplateTargetItem.Value>\#region ISerializable
// Implement this method to serialize data. The method is called on serialization.
public void GetObjectData(SerializationInfo info, StreamingContext context)
{
.foreach_item(Attributes,\n)
.if(%ArraySize%&gt;1)
     info.AddValue(&quot;%.T:Code%&quot;, %.T:Code%, typeof(%DataType%\[\]));
.else
     info.AddValue(&quot;%.T:Code%&quot;, %.T:Code%, typeof(%DataType%));
.endif
.next(\n)

}
\#endregion // ISerializable

\#region Serialization
/// &lt;summary&gt;
/// Initialise l&#39;instance depuis les données d&#39;un flux binaire
/// &lt;/summary&gt;
/// &lt;param name=&quot;reader&quot;&gt;Flux binaire&lt;/param&gt;
/// &lt;param name=&quot;aggregationCallback&quot;&gt;Permet d&#39;appliquer des modifications aux entités importées par aggrégation&lt;/param&gt;
/// &lt;remarks&gt;Seuls les éléments existants dans le noeud Xml son importés dans l&#39;objet&lt;/remarks&gt;
public void ReadBinary(BinaryReader reader, EntityCallback aggregationCallback)
{
   // Properties\n
.foreach_item(Attributes)
.if(%ArraySize%&gt;1)
   %.T:Code% = new %baseDataType%\[%ArraySize%\];
   for(int i=0;i&lt;%ArraySize%;i++)\n
.if(%DataType%==&quot;DateTime&quot;)
      %.T:Code%\[i\] = %toForcedDataType% new DateTime(reader.ReadInt64());
.else
      %.T:Code%\[i\] = %toForcedDataType% reader.Read%.FU:baseDataType%();
.endif
.else
.if(%DataType%==&quot;DateTime&quot;)
   %.T:Code% = %toForcedDataType% new DateTime(reader.ReadInt64());
.else
   %.T:Code% = %toForcedDataType% reader.Read%.FU:baseDataType%();
.endif
.endif
.next(\n)

.foreach_item(Associations,\n)
.if(%IsComposition%==true)
   .set_value(assName,null)
   .set_object(assClass,null)
   .if(%thisCode%==%ClassA.Code%) and (%Container%==&quot;A&quot;)
   .set_value(assName,%roleBName%)
   .set_object(assClass,%ClassB%)
   .elsif(%thisCode%==%ClassB.Code%) and (%Container%==&quot;B&quot;)
   .set_value(assName,%roleAName%)
   .set_object(assClass,%ClassA%)
   .endif
   .if(%assName%!=null) and (%assClass%!=null)

   // %assName%\n
   .if((%thisCode%==%ClassA.Code%) and (%RoleBMultiple%==true)) or ((%thisCode%==%ClassB.Code%) and (%RoleAMultiple%==true))
   {
      int size = reader.ReadInt32();
      if (size &gt; 0)
      {
          //this.%.FU:assName% = new Collection&lt;%assClass.Code%&gt;();
          for(int i=0;i&lt;size;i++){
              %assClass.Code% o = new %assClass.Code%();
              this.Model.Add(o);
              o.ReadBinary(reader, aggregationCallback);
              if (aggregationCallback != null)
                 aggregationCallback(o);
              this.Add%.FU:assName%(o);
          }
      }
      //else
      //{
      //    this.%.FU:assName% = new Collection&lt;%assClass.Code%&gt;();
      //}
   }
   .else
   {
      bool exists = reader.ReadBoolean();
      if (exists)
      {
         this.%.FU:assName% = new %assClass.Code%();
         this.Model.Add(this.%.FU:assName%);
         this.%.FU:assName%.ReadBinary(reader, aggregationCallback);
      }
      else
      {
          this.%.FU:assName% = null;
      }
   }
   .endif
   .endif
.endif
.next

}

public void WriteBinary(BinaryWriter writer)
{
   // Properties
.foreach_item(Attributes,\n)
.if(%ArraySize%&gt;1)
   for(int i=0;i&lt;%ArraySize%;i++)\n
.if(%DataType%==&quot;DateTime&quot;)
      writer.Write(%.T:Code%\[i\]
.if(%isNullableBase%)
\.Value
.endif
\.Ticks);
.else
      writer.Write(%fromForcedDataType%%.T:Code%\[i\]
.if(%isNullableBase%)
\.Value
.endif
);
.endif
.else
.if(%DataType%==&quot;DateTime&quot;)
   writer.Write(%.T:Code%
.if(%isNullableBase%)
\.Value
.endif
\.Ticks);
.else
   writer.Write(%fromForcedDataType%%.T:Code%
.if(%isNullableBase%==true)
\.Value
.endif
);
.endif
.endif
.next(\n)

.foreach_item(Associations,\n)
.if(%IsComposition%==true)
   .set_value(assName,null)
   .set_object(assClass,null)
   .if(%thisCode%==%ClassA.Code%) and (%Container%==&quot;A&quot;)
   .set_value(assName,%roleBName%)
   .set_object(assClass,%ClassB%)
   .elsif(%thisCode%==%ClassB.Code%) and (%Container%==&quot;B&quot;)
   .set_value(assName,%roleAName%)
   .set_object(assClass,%ClassA%)
   .endif
   .if(%assName%!=null) and (%assClass%!=null)

   // %assName%\n
   .if((%thisCode%==%ClassA.Code%) and (%RoleBMultiple%==true)) or ((%thisCode%==%ClassB.Code%) and (%RoleAMultiple%==true))
   List&lt;%assClass.Code%&gt; %.L:assName% = this.%.FU:assName%.ToList();
   writer.Write(%.L:assName%.Count);
   if (%.L:assName%.Count &gt; 0)
   {
       foreach (var col in %.L:assName%)
           col.WriteBinary(writer);
   }
   .else
   writer.Write((this.%.L:assName% == null) ? false : true);
   if (this.%.L:assName% != null)
   {
       this.%.L:assName%.WriteBinary(writer);
   }
   .endif
   .endif
.endif
.next

}


/// &lt;summary&gt;
/// Convertie l&#39;instance en élément XML
/// &lt;/summary&gt;
/// &lt;param name=&quot;parent&quot;&gt;Élément parent reçevant le nouveau noeud&lt;/param&gt;
/// &lt;returns&gt;Text XML du document&lt;/returns&gt;
public string ToXml(XmlElement parent)
{
   XmlElement curMember = null;
   XmlDocument doc = null;
   // Element parent ?
   if (parent != null)
   {
       doc = parent.OwnerDocument;
   }
   else
   {
       doc = new XmlDocument();
       parent = doc.CreateElement(&quot;root&quot;);
       doc.AppendChild(parent);
   }

   //Ecrit au format XML
   XmlElement cur = doc.CreateElement(&quot;%Code%&quot;);
   parent.AppendChild(cur);
       
   //
   // Fields
   //
   
.foreach_item(Attributes,\n)
   // Assigne le membre %.FU:Code%

.if(%isNullable%==true)
   if (%.L:Code% != null)
   {
       curMember = doc.CreateElement(&quot;%.FU:Code%&quot;);
       curMember.AppendChild(doc.CreateTextNode(%.L:Code%.ToString()));
       cur.AppendChild(curMember);
   }
.else
   curMember = doc.CreateElement(&quot;%.FU:Code%&quot;);
   curMember.AppendChild(doc.CreateTextNode(%.L:Code%.ToString()));
   cur.AppendChild(curMember);
.endif(\n)

.next(\n)
   
   //
   // Compositions
   //
.foreach_item(Associations,\n)
.if(%IsComposition%==true)
   .set_value(assName,null)
   .set_object(assClass,null)
   .if(%thisCode%==%ClassA.Code%) and (%Container%==&quot;A&quot;)
   .set_value(assName,%roleBName%)
   .set_object(assClass,%ClassB%)
   .elsif(%thisCode%==%ClassB.Code%) and (%Container%==&quot;B&quot;)
   .set_value(assName,%roleAName%)
   .set_object(assClass,%ClassA%)
   .endif
   .if(%assName%!=null) and (%assClass%!=null)

   // %assName%\n
   .if((%thisCode%==%ClassA.Code%) and (%RoleBMultiple%==true)) or ((%thisCode%==%ClassB.Code%) and (%RoleAMultiple%==true))
   {
      List&lt;%assClass.Code%&gt; %.L:assName% = this.%.FU:assName%.ToList();
      curMember = doc.CreateElement(&quot;%assName%&quot;);
      if (%.L:assName%.Count &gt; 0)
      {
          foreach (var col in %.L:assName%)
              col.ToXml(curMember);
      }
      cur.AppendChild(curMember);
   }
   .else
   if(this.%.L:assName% != null)
      this.%.L:assName%.ToXml(cur);
   .endif
   .endif
.endif
.next(\n)


   parent.AppendChild(cur);
   return doc.InnerXml;
}

/// &lt;summary&gt;
/// Initialise l&#39;instance depuis des données XML
/// &lt;/summary&gt;
/// &lt;param name=&quot;element&quot;&gt;Élément contenant les information sur l&#39;objet&lt;/param&gt;
/// &lt;param name=&quot;aggregationCallback&quot;&gt;Permet d&#39;appliquer des modifications aux entités importées par aggrégation&lt;/param&gt;
/// &lt;remarks&gt;Seuls les éléments existants dans le noeud Xml son importés dans l&#39;objet&lt;/remarks&gt;
public void FromXml(XmlElement element, EntityCallback aggregationCallback)
{
   foreach (XmlElement m in element.ChildNodes)
   {
       string property_value = m.InnerText.Trim();
       // charge les paramètres
       switch (m.Name)
       {
         //
         // Fields
         //

.foreach_item(Attributes,\n)
         // Assigne le membre %.FU:Code%
         case &quot;%.FU:Code%&quot;:
         {

.if(%.L:DataType%==&quot;string&quot;)
            this.%.L:Code% = property_value;
.else
            %DataType% value;
            if(%DataType%.TryParse(property_value,out value)==false)
               this.%.FU:Code% = %allocation%;
            else
               this.%.FU:Code% = %toForcedDataType%value;
.endif

         }
         break;
.next(\n)


         //
         // Compositions
         //
         
.foreach_item(Associations,\n)
.if(%IsComposition%==true)
   .set_value(assName,null)
   .set_object(assClass,null)
   .if(%thisCode%==%ClassA.Code%) and (%Container%==&quot;A&quot;)
   .set_value(assName,%roleBName%)
   .set_object(assClass,%ClassB%)
   .elsif(%thisCode%==%ClassB.Code%) and (%Container%==&quot;B&quot;)
   .set_value(assName,%roleAName%)
   .set_object(assClass,%ClassA%)
   .endif
   .if(%assName%!=null) and (%assClass%!=null)
   .if((%thisCode%==%ClassA.Code%) and (%RoleBMultiple%==true)) or ((%thisCode%==%ClassB.Code%) and (%RoleAMultiple%==true))
         // Assigne la collection %assName%
         case &quot;%assName%&quot;:
            {
               foreach (XmlElement c in m.ChildNodes)
               {
                  if(&quot;%assClass.Code%&quot; == c.Name){
                      %assClass.Code% value = new %assClass.Code%();
                      value.FromXml(c, aggregationCallback);
                      if (aggregationCallback != null)
                         aggregationCallback(value);
                      this.Add%.FU:assName%(value);
                  }
               }
            }
            break;
   .else
         // Assigne le membre %assName%
         case &quot;%assName%&quot;:
            {
               this.%.L:assName% = new %assClass.Code%();
               this.%.L:assName%.FromXml(m, aggregationCallback);
            }
            break;
   .endif
   .endif
.endif
.next(\n)

			}
   }
}

