   .if(%assName%!=null) and (%assClass%!=null)

   // %assName%\n
   .if((%thisCode%==%ClassA.Code%) and (%RoleBMultiple%==true)) or ((%thisCode%==%ClassB.Code%) and (%RoleAMultiple%==true))
   {
      curMember = doc.CreateElement(&quot;%assName%&quot;);
      if (this.%.L:assName%.Count &gt; 0)
      {
          foreach (var col in this.%.L:assName%)
              col.ToXml(curMember);
      }
      cur.AppendChild(curMember);
   }
   .else
   this.%.L:assName%.ToXml(cur);
   .endif
   .endif
.endif
.next(\n)


   parent.AppendChild(cur);
   return doc.InnerXml;
}

/// &lt;summary&gt;
