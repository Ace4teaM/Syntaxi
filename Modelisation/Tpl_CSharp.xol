   .set_value(assName,%roleAName%)
   .set_object(assClass,%ClassA%)
   .endif
   .if(%assName%!=null) and (%assClass%!=null)
   .if((%thisCode%==%ClassA.Code%) and (%RoleBMultiple%==true)) or ((%thisCode%==%ClassB.Code%) and (%RoleAMultiple%==true))
         // Assigne la collection %assName%
         case &quot;%assName%&quot;:
            {
               foreach (XmlElement c in m.ChildNodes)
               {
                  if(&quot;%assClass.Code%&quot; == m.Name){
                      %assClass.Code% value = new %assClass.Code%();
                      value.FromXml(c);
                      this.Add%assName%(value);
                  }
               }
            }
            break;
   .else
         // Assigne le membre %assName%
         this.%.L:assName%.FromXml(m);
   .endif
   .endif
.endif
.next(\n)

			}
   }
}

