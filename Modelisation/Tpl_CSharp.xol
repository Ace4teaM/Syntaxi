public void FromXml(XmlElement element)
{
   foreach (XmlElement m in element.ChildNodes)
   {
       string property_value = m.InnerText.Trim();
       // charge les paramètres
       switch (m.Name)
       {
         //
         // Fields
         //

.foreach_item(Attributes,\n)
         // Assigne le membre %.FU:Code%
         case &quot;%.FU:Code%&quot;:
         {

.if(%.L:DataType%==&quot;string&quot;)
            this.%.L:Code% = property_value;
.else
            %DataType% value;
