/// &lt;param name=&quot;parent&quot;&gt;Élément parent reçevant le nouveau noeud&lt;/param&gt;
/// &lt;returns&gt;Text XML du document&lt;/returns&gt;
public string ToXml(XmlElement parent)
{
   XmlElement curMember = null;
   XmlDocument doc = null;
   // Element parent ?
   if (parent != null)
   {
       doc = parent.OwnerDocument;
   }
   else
   {
       doc = new XmlDocument();
       parent = doc.CreateElement(&quot;root&quot;);
       doc.AppendChild(parent);
   }

   //Ecrit au format XML
   XmlElement cur = doc.CreateElement(&quot;%Code%&quot;);
   parent.AppendChild(cur);
       
   //
   // Fields
   //
   
.foreach_item(Attributes,\n)
		// Assigne le membre %.FU:Code%

.if(%isNullable%==true)
   if (%.L:Code% != null)
   {
       curMember = doc.CreateElement(&quot;%.FU:Code%&quot;);
       curMember.AppendChild(doc.CreateTextNode(%.L:Code%.ToString()));
       cur.AppendChild(curMember);
   }
.else
   curMember = doc.CreateElement(&quot;%.FU:Code%&quot;);
   curMember.AppendChild(doc.CreateTextNode(%.L:Code%.ToString()));
   cur.AppendChild(curMember);
.endif(\n)

.next(\n)
   
   //
